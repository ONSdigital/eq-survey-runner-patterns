<!-- Favicon setup -->
  <!-- icon in the highest resolution we need it for -->
  <link rel="icon" type="image/png" sizes="192x192" href="{{ path '/assets/favicons/android-chrome-192x192.png'}}">
  <!-- reuse same icon for Safari -->
  <link rel="apple-touch-icon" href="{{ path '/assets/favicons/apple-touch-icon.png'}}">
  <!-- multiple icons for IE & Windows phone -->
  <meta name="msapplication-TileColor" content="#033E58">
  <meta name="msapplication-square70x70logo" content="{{ path '/assets/favicons/mstile-70x70.png'}}">
  <meta name="msapplication-square70x70logo" content="{{ path '/assets/favicons/mstile-144x144.png'}}">
  <meta name="msapplication-square150x150logo" content="{{ path '/assets/favicons/mstile-150x150.png'}}">
  <meta name="msapplication-wide310x150logo" content="{{ path '/assets/favicons/mstile-310x150.png'}}">
  <meta name="msapplication-square310x310logo" content="{{ path '/assets/favicons/mstile-310x310.png'}}">
  <link rel="mask-icon" href="{{ path '/assets/favicons/safari-pinned-tab.svg'}}" color="#033E58">
  <link rel="manifest" href="{{ path '/assets/favicons/site.webmanifest'}}">
  <!-- /Favicon setup -->
